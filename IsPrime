package week1.day2;

public class IsPrime {

	public static void main(String[] args) {
		
		int n =9;
		boolean result = false;
		
		for(int i=2; i<n-1;i++)
		{
			if(n%i==0)
				
			
			{
			result = true;
					break;
			}
		}
	
	
		
		
			if(result==false)
			
				System.out.println(n +  " Prime");
			
			else
				
				System.out.println(n + " Not Prime");
	}
}

		

	

